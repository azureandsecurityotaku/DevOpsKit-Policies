{
  "mode": "All",
   "policyRule": {
      "if": {
        "allOf": [
          {
            "field": "type",
            "equals": "Microsoft.ApiManagement/service/products"
          },
          {
            "field": "Microsoft.ApiManagement/service/products/subscriptionRequired",
            "equals": "false"
          }
        ]
      },
      "then": {
        "effect": "audit"
      }
    },
  "parameters": {}
}